  "devDependencies": {
    "enzyme": "^3.10.0",
    "eslint-plugin-react": "^7.17.0",
    "gh-pages": "^2.1.1",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexbarkin/alexbarkin.github.io"
  },
