{
  "name": "alex-barkin-website",
  "homepage": "https://alexbarkin.github.io",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "test": "jest",
    "lint": "eslint src/**/**.{js,jsx}",
    "check-prettier": "prettier --check \"**/*.{js,jsx,json,md}\"",
    "coverage": "jest src --coverage"
  },
  "dependencies": {
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.20",
    "bootstrap": "^4.3.1",
    "bootstrap-table": "^1.15.5",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.x",
    "gatsby": "^2.17.2",
    "gatsby-image": "^2.2.27",
    "gatsby-plugin-manifest": "^2.2.26",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.2.32",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-transformer-remark": "^2.6.30",
